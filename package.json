{
  "name": "coursera_django_customer_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "claude": "claude",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "schema-to-zod": "openapi-zod-client ./docs/schema.yml --output ./src/generatedtypes/django_generated.ts --with-docs --with-description  -p ./.prettierrc && ./cleanup_django_generated.sh",
    "schema-zod-to-json": "tsx generate-schema-types.ts",
    "schema-json-to-ts": "json2ts -i ./docs/schema.json -o ./src/types/django_api_types.ts",
    "schema": "pnpm run schema-to-zod && pnpm run schema-zod-to-json && pnpm run schema-json-to-ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@tanstack/react-query": "^5.81.2",
    "@zodios/core": "^10.9.6",
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.25.67",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.35",
    "@eslint/js": "^9.25.0",
    "@redux-devtools/extension": "^3.3.0",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "openapi-zod-client": "^1.18.3",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "zod-to-json-schema": "^3.24.6"
  }
}
